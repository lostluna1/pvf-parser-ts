{
  "compilerOptions": {
    "target": "ES2020",
  "module": "Node16",
  "moduleResolution": "node16",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "jsx": "react-jsx",
  "types": ["node", "vscode", "react", "react-dom"]
  },
  "include": ["src/**/*"]
}
